{
    "version": 2,
     "builds": [
        {
            "src": "app.js",
            "env": {
                "MONGO_URI": "@mongo_uri"
            },
            "output": "standalone",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
  }