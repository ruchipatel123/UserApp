{
    "version": 2,
    "functions": {
        "api/index.js": {
            "maxDuration": 10000
        }
    },
    "routes": [
        {
            "src": "/public/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}